{
  "name": "nerd-components/common-functions",
  "description": "A lot of functions used in Nerd Framework applications.",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "files": [
      "src/Nerd/Common/Arrays.php",
      "src/Nerd/Common/Classes.php",
      "src/Nerd/Common/Strings.php",
      "src/Nerd/Common/Functional.php"
    ],
    "psr-4": {
      "tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "satooshi/php-coveralls": "^1.0",
    "squizlabs/php_codesniffer": "^2.7"
  }
}
